<td data-th="HTTP Status Code"  width='15%' class='code'>
    {{{escape code}}}
</td>
<td data-th="Reason" class="markdown">
    {{{escape message}}}
</td>
<td data-th="Response Model" width='50%' class="model">
    <span class="model-signature" />
</td>
<td data-th="Headers" class="headers">
        <table>
            <tbody>
            {{#each headers}}
                <tr>
                    <td>{{@key}}</td>
                    <td>{{{sanitize this.description}}}</td>
                    <td>{{{escape this.type}}}</td>
                </tr>
            {{/each}}
            </tbody>
        </table>
</td>
